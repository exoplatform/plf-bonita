<page-set
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.gatein.org/xml/ns/gatein_objects_1_1_1 http://www.gatein.org/xml/ns/gatein_objects_1_1_1"
    xmlns="http://www.gatein.org/xml/ns/gatein_objects_1_1_1">

	<page>
		<name>workflow</name>
		<title>workflow</title>
		<access-permissions>member:/platform/users</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>bonita-portlet</application-ref>
				<portlet-ref>UIParametrizedIFramePortlet</portlet-ref>
			</portlet>
			<title>workflow</title>
			<access-permissions>member:/platform/users</access-permissions>
			<show-info-bar>false</show-info-bar>
			<height>450px</height>
		</portlet-application>
		<container id="ecmexplorer" template="system:/groovy/portal/webui/container/UIWfContainer.gtmpl">
        <access-permissions>Everyone</access-permissions>
		 <portlet-application>
		    <portlet>
		        <application-ref>ecmexplorer</application-ref>
		        <portlet-ref>FileExplorerPortlet</portlet-ref>
		        <preferences>
		    		<preference>
		    			<name>driveName</name>     
		    			<value>collaboration</value>        
		    			<read-only>false</read-only>
		    		</preference>        	
		    		<preference>
		    			<name>showActionBar</name>     
		    			<value>false</value>        
		    			<read-only>false</read-only>
		    		</preference>        	
		    		<preference>
		    			<name>editInNewWindow</name>     
		    			<value>false</value>        
		    			<read-only>false</read-only>
		    		</preference>        	
		    		<preference>
		    			<name>usecase</name>     
		    			<value>selection</value>        
		    			<read-only>false</read-only>
		    		</preference>        	
		    		<preference>
		    			<name>showSideBar</name>     
		    			<value>false</value>        
		    			<read-only>false</read-only>
		    		</preference>        	
		    		<preference>
		    			<name>showTopBar</name>     
		    			<value>false</value>        
		    			<read-only>false</read-only>
		    		</preference>        	
		        </preferences>
		      </portlet>
		      <title>workflow</title>
		      <access-permissions>member:/platform/users</access-permissions>
		      <show-info-bar>false</show-info-bar>
        </portlet-application>
       </container> 
	</page>
	
	<page>
		<name>bonitaXP</name>
		<title>bonitaXP</title>
		<access-permissions>member:/platform/users</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>bonita-portlet</application-ref>
				<portlet-ref>UIIFrameLocalPortlet</portlet-ref>
				<preferences>
					<preference>
						<name>url</name>
						<value>/bonita</value>
						<read-only>false</read-only>
					</preference>
				</preferences>
			</portlet>
			<title>bonitaXP</title>
			<access-permissions>member:/platform/users</access-permissions>
			<show-info-bar>false</show-info-bar>
			<height>640px</height>
		</portlet-application>
	</page>
	
	<page>
		<name>bonitaTODO</name>
		<title>bonitaTODO</title>
		<access-permissions>member:/platform/users</access-permissions>
		<edit-permission>manager:/platform/administrators</edit-permission>
		<portlet-application>
			<portlet>
				<application-ref>bonita-portlet</application-ref>
				<portlet-ref>IFrameLocalPortlet</portlet-ref>
				<preferences>
					<preference>
						<name>url</name>
						<value>/bonita/console/BonitaConsole.html?#CaseList/lab:Inbox</value>
						<read-only>false</read-only>
					</preference>
				</preferences>
			</portlet>
			<title>bonitaTODO</title>
			<access-permissions>member:/platform/users</access-permissions>
			<show-info-bar>false</show-info-bar>
			<height>640px</height>
		</portlet-application>
	</page>
  
</page-set>
